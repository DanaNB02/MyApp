[
  {
    "text": "فمرّ فأرٌ صغير فوقه فأيقظه",
    "start": 2.785958,
    "end": 7.760408
  },
  {
    "text": "أمسك الأسد بالفأر",
    "start": 8.376188,
    "end": 9.98473
  },
  {
    "text": "وتوسل إليه ليتركه قائلًا: ‘قد أساعدك يومًا ما’",
    "start": 10.239742,
    "end": 14.614192
  },
  {
    "text": "ضحك الأسد وأطلق سراحه",
    "start": 15.300765,
    "end": 17.517414
  },
  {
    "text": "وبعد أيام وقع الأسد في شبكة صيادين",
    "start": 18.066672,
    "end": 21.6957
  },
  {
    "text": " فجاء الفأر وقطع الحبال بأسنانه",
    "start": 22.34304,
    "end": 25.422809
  },
  {
    "text": "لينقذ الأسد",
    "start": 25.873985,
    "end": 26.952885
  }
