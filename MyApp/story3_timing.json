    [
      {
        "text": "فكر الحكيم كيف ينصح أهل القرية",
        "start": 12.620602,
        "end": 15.26122
      },
      {
        "text": "حتى أتته فكرة",
        "start": 15.766044,
        "end": 17.552345
      },
      {
        "text": "فضحكوا جميعًا بشدة وبصوتٍ عالٍ",
        "start": 23.843229,
        "end": 27.41583
      },
      {
        "text": " فابتسم بعضهم",
        "start": 34.211539,
        "end": 35.726011
      },
      {
        "text": "فلم يهتم أو يبتسم أحد",
        "start": 40.929582,
        "end": 43.376037
      },
      {
        "text": "لا يمكنكم الضحك على نفس المزحة مرارًا وتكرارًا",
        "start": 47.531128,
        "end": 51.181394
      },
      {
        "text": " لكنكم لا تكفون عن البكاء على نفس المشاكل مرارًا وتكرارًا!",
        "start": 52.191042,
        "end": 57.355781
      }
    ]
